    "prepublish": "npm run -s build",
    "install": "node tools/unicode-category-parser.js"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "eslint": "^5.8.0",
    "eslint-config-metarhia": "^6.0.0",
    "eslint-plugin-import": "^2.14.0",
    "metaschema": "^0.0.21",
    "metasync": "^0.3.29",
    "metatests": "^0.2.3"
  }
}

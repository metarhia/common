{
  "name": "@metarhia/common",
  "version": "1.2.1",
  "author": "Timur Shemsedinov <timur.shemsedinov@gmail.com>",
  "description": "Metarhia Common Library",
  "license": "MIT",
  "keywords": [
    "metarhia",
    "common",
    "impress",
    "jstp",
    "globalstorege",
    "metasync"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/metarhia/common"
  },
  "main": "./common.js",
  "browser": {
    "common.js": "./dist/common.js"
  },
  "readmeFilename": "README.md",
  "files": [
    ".babelrc",
    "common.js",
    "lib/",
    "dist/"
  ],
  "scripts": {
    "test": "eslint . && metatests test/",
    "lint": "eslint . && prettier -c \"**/*.js\" \"**/*.json\" \"**/*.md\" \".*rc\" \"**/*.yml\"",
    "fmt": "prettier --write \"**/*.js\" \"**/*.json\" \"**/*.md\" \".*rc\" \"**/*.yml\"",
    "docs": "metaschema ./common.js",
    "build": "babel common.js -d dist && babel lib -d dist/lib",
    "prepublish": "npm run -s build"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "eslint": "^5.9.0",
    "eslint-config-metarhia": "^6.0.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.1",
    "metaschema": "0.0.28",
    "metasync": "^0.3.31",
    "metatests": "^0.3.0",
    "prettier": "1.16.x"
  }
}
